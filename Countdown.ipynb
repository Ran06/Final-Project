   "source": [
    "import time\n",
    "n = 60\n",
    "while n > 0 : \n",
    "    mins, secs = divmod(n, 60)\n",
    "    hours, mins = divmod(mins, 60)\n",
    "    print(f'{hours:02d}:{mins:02d}:{secs:02d}', end = '\\r')\n",
    "    time.sleep(1)\n",
    "    n -= 1\n",
    "n"
   ]
